<div>
  <h3>{{user.floor}}층 사생 명단</h3>
  <table border="1" cellpadding="10" id="resi-list-id">
    <thead>
      <tr>
        <th>방</th>
        <th>이름</th>
      </tr>

    </thead>
    <tbody>
      {% for resident in residents %}
      <tr>
        <td>{{resident.room}}</td>
        <td>{{resident.name}}</td>
        <td>
          <button type="button" id="resi-del-btn" value="삭제" onclick="delRow( '{{resident.id}}', this )">삭제</button>
        </td>
        <td>
          <label for="in{{loop.index}}"><input type="checkbox" id="in{{loop.index}}" name="in" value='재실'
              onclick='getInCheckboxCnt(event)' />
            재실
          </label>
        </td>
        <td>
          <label for="out{{loop.index}}"><input type="checkbox" id="out{{loop.index}}" name="out" value='무단'
              onclick='getOutCheckboxCnt(event)' />
            무단
          </label>
        </td>
        <td>{{loop.index}}</td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
  <div>
    재실 <span id="resultIn">0</span>
  </div>
  <div>
    무단 <span id="resultOut">0</span>
  </div>
</div>