<div>
  <h3>{{user.floor}}층 사생 명단</h3>
  <table border="1" cellpadding="10" id="resi-list-id">
    <thead>
      <tr>
        <th>방</th>
        <th>이름</th>
      </tr>

    </thead>
    <tbody>
      {% for resident in residents %}
      <tr>
        <td>{{resident.room}}</td>
        <td>{{resident.name}}</td>
        <td>
          <button type="button" id="resi-del-btn" value="삭제" onclick="delRow( '{{resident.id}}', this )">삭제</button>
        </td>
        <td>
          <label for="{{loop.index}}"><input type="checkbox" id="{{loop.index}}" name="in" value='재실'
              onclick='getCheckboxCnt(event)' />
            재실</label>
        </td>
        <td>{{loop.index}}</td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
  <div id='result'></div>
</div>