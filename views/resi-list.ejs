<div>
  <h3>{{user.floor}}층 사생 명단</h3>
  <table border="1" cellpadding="10" id="resi-list-id">
    <thead>
      <tr>
        <th>방</th>
        <th>이름</th>
      </tr>
      {% for resident in residents %}
      <tr>
        <td>{{resident.room}}</td>
        <td>{{resident.name}}</td>
      </tr>
      {% endfor %}
    </thead>
    <tbody></tbody>
  </table>
</div>